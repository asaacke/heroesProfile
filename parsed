<script>

$('#timeframe').change(function(){



  $('#type-label').show();

  $('#type-label select').prop('disabled', false);





  if($('#timeframe').find(":selected").val() == "2.41"){

    $('#type-label').hide();

    $('#type-label select').attr('disabled', 'disabled');



  }else if($('#timeframe').find(":selected").val() == "2.42"){

      $('#type-label').hide();

      $('#type-label select').attr('disabled', 'disabled');



    }

});





$('#timeframe_type').change(function(){

if($('#timeframe_type').find(":selected").val() == "minor"){

  $('#minor_timeframe-label').show();

  $('#minor_timeframe').prop('disabled', false);

  $('#minor_timeframe-label .ms-choice').removeClass('disabled');



  /*

  if($("#minor_timeframe-label .ms-drop ul li:nth-child(2) input").prop('checked') != true){

    $('#minor_timeframe-label .ms-drop ul li:nth-child(2) input').trigger('click');

  }

  */



  $('#major_timeframe-label').hide();

  $('#major_timeframe').prop('disabled', true);





  $('#days_timeframe-label').hide();

  $('#days_timeframe').prop('disabled', true);



}else if($('#timeframe_type').find(":selected").val() == "major"){

  $('#major_timeframe-label').show();

  $('#major_timeframe').prop('disabled', false);

  $('#major_timeframe-label .ms-choice').removeClass('disabled');



  /*

  if($("#major_timeframe-label .ms-drop ul li:nth-child(2) input").prop('checked') != true){

    $('#major_timeframe-label .ms-drop ul li:nth-child(2) input').trigger('click');

  }

  */

  $('#minor_timeframe-label').hide();

  $('#minor_timeframe').prop('disabled', true);



  $('#days_timeframe-label').hide();

  $('#days_timeframe').prop('disabled', true);



}else if($('#timeframe_type').find(":selected").val() == "days"){

  $('#days_timeframe-label').show();

  $('#days_timeframe').prop('disabled', false);

  $('#days_timeframe-label .ms-choice').removeClass('disabled');





  $('#minor_timeframe-label').hide();

  $('#minor_timeframe').prop('disabled', true);



  $('#major_timeframe-label').hide();

  $('#major_timeframe').prop('disabled', true);

}else if($('#timeframe_type').find(":selected").val() == "last_update"){

  $('#minor_timeframe-label').hide();

  $('#major_timeframe-label').hide();



}



});



</script>
