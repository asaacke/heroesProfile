window.top.__vm_add = window.top.__vm_add || [];







function squareAd(element){

    var dynamicBanner = document.createElement('div');

  dynamicBanner.setAttribute('class', 'vm-placement');

  if (breakpointTablet == true){

    dynamicBanner.setAttribute('data-id', '60f5939d46e4640fd9497d35'); // Mobile Dynamic Banner

  }

  else{



    dynamicBanner.setAttribute('data-id', '60f5939d46e4640fd9497d35'); // Desktop Dynamic Banner



  }

  element.append(dynamicBanner);

  window.top.__vm_add.push(dynamicBanner);



}



function dynamicAd(element){

    var dynamicBanner = document.createElement('div');

  dynamicBanner.setAttribute('class', 'vm-placement');

  if (breakpointTablet == true){

    dynamicBanner.setAttribute('data-id', '60f593cedd63d722e7e57bc8'); // Mobile Dynamic Banner

  }

  else{



    dynamicBanner.setAttribute('data-id', '60f593ac46e4640fd9497d39'); // Desktop Dynamic Banner



  }

  element.append(dynamicBanner);

  window.top.__vm_add.push(dynamicBanner);



}





function horizontalAd(element){

    var dynamicBanner = document.createElement('div');

  dynamicBanner.setAttribute('class', 'vm-placement');

  if (breakpointTablet == true){

    dynamicBanner.setAttribute('data-id', '60f593cedd63d722e7e57bc8'); // Mobile Dynamic Banner

  }

  else{



    dynamicBanner.setAttribute('data-id', '60f593a446e4640fd9497d37'); // Desktop static Banner



  }

  element.append(dynamicBanner);

  window.top.__vm_add.push(dynamicBanner);



}







