      function sortTable(f,n){

      	var rows = $('#map-data tbody  tr').get();



      	rows.sort(function(a, b) {



      		var A = getVal(a);

      		var B = getVal(b);



          if (typeof A === 'string' || A instanceof String){

            if(A < B) {

              return -1*f;

            }

            if(A > B) {

              return 1*f;

            }

            return 0;

          }

          else{

            return f*(A - B);

          }

      	});



      	function getVal(elm){

      		var v = $(elm).children('td').eq(n).text().toUpperCase().replace(',', '');



      		if($.isNumeric(v)){

if(v < 0){

  v =parseFloat(v, 10).toFixed(2)*-1*-1;

  //console.log(v,parseFloat(v, 10).toFixed(2));

}

else if((v % 1) == 0){

              v = parseInt(v,10);



            }else{

              v = parseFloat(v,10).toFixed(2);

            }

            ////console.log(v);

      		}

      		return v;

      	}



      	$.each(rows, function(index, row) {

      		$('#map-data').children('tbody').append(row);

      	});

      }

