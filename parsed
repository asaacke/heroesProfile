      $('.leaderboard').submit(function(e){



        e.preventDefault();

        var timeframeValues = "";

        var regionValues = "";



        if($('#timeframe_type').find(":selected").val() == "minor"){

          var timeframeValues = $('#minor_timeframe').val();



        }else if($('#timeframe_type').find(":selected").val() == "major"){

          var timeframeValues = $('#major_timeframe').val();



        }else if($('#timeframe_type').find(":selected").val() == "days"){

          var timeframeValues = $('#days_timeframe').val();

        }



        if( $('[data-name="selectAllregion[]"]').parent().parent().hasClass('selected') === false){

           regionValues = "&region="+Array.from(document.getElementById("region").options).filter(option => option.selected).map(option => option.value).join();

        }



   var leagueTierValues = '';

   var heroLeagueTierValues = '';

   var roleLeagueTierValues = '';

   var gametypeValues = Array.from(document.getElementById("game_type").options).filter(option => option.selected).map(option => option.value);

   var heroLevelValues = '';

   var heroValue = $('#hero').val();



   var mirrorValue = "";

   if($('#mirror').val() != ""){

     var mirrorValue = "&mirror="+$('#mirror').val();

   }



   var timeFrameType = $('#timeframe_type').find(":selected").val();



   if( $('[data-name="selectAllhero_level[]"]').parent().parent().hasClass('selected') === false){

      heroLevelValues = "&hero_level="+Array.from(document.getElementById("hero_level").options).filter(option => option.selected).map(option => option.value).join();

   }



   if( $('[data-name="selectAllleague_tier[]"]').parent().parent().hasClass('selected') === false){

      leagueTierValues = "&league_tier=" + Array.from(document.getElementById("league_tier").options).filter(option => option.selected).map(option => option.value).join();

   }

   if( $('[data-name="selectAllhero_league_tier[]"]').parent().parent().hasClass('selected') === false){

      heroLeagueTierValues = "&hero_league_tier=" + Array.from(document.getElementById("hero_league_tier").options).filter(option => option.selected).map(option => option.value).join();

   }



   if( $('[data-name="selectAllrole_league_tier[]"]').parent().parent().hasClass('selected') === false){

      roleLeagueTierValues = "&role_league_tier=" + Array.from(document.getElementById("role_league_tier").options).filter(option => option.selected).map(option => option.value).join();

   }



   window.location ='/Global/Hero/Maps/?hero='+heroValue+'&timeframe_type='+timeFrameType+"&timeframe="+timeframeValues+regionValues+heroLevelValues+"&game_type="+gametypeValues.join()+leagueTierValues+heroLeagueTierValues+roleLeagueTierValues+mirrorValue;



      });









