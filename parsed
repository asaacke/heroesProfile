        f_popularity_column = 1;

        f_games_played_column = 1;

            f_ban_rate_column = 1;

            f_win_rate_column = 1;

            f_win_column = 1;

            f_loss_column = 1;

            f_type_column = 1;

            f_change_column = 1;

          f_hero_column *= -1;

          if(f_hero_column == -1){

            $(this).find('i').addClass('fa-sort-down');

        }

        else{

          $(this).find('i').addClass('fa-sort-up');



        }

          var n = $(this).prevAll().length;

          sortTable(f_hero_column,n);

