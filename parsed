<script>

var modal = document.getElementById('heroesprofile-alert2');

/* if they haven't been to the site before, show the modal window.*/



function displayLoading(){



  $("#loading-div").css('display', 'flex');



}



function hideLoading(){

$("#loading-div").hide();

}







hideLoading();

window.onpageshow = function(event) {

    if (event.persisted) {

        window.location.reload()

    }

};



$(document).ready(function(){



  $('form').submit(function(){

    displayLoading();

  });

  $('.flyout-menu a').click(function(){

    displayLoading();

  });



  function localStorageExist(){

      var test = 'test';

      try {

          localStorage.setItem(test, test);

          localStorage.removeItem(test);

          return true;

      } catch(e) {

          return false;

      }

  }



  if(localStorageExist() === true){

    if(!localStorage.getItem("uploaderTimestamp1") || localStorage.getItem("uploaderTimestamp1") <= todaysdate){

      if(localStorage.getItem("uploaderAlert1") != "true"){

        var mac = /(Mac|iPhone|iPod|iPad)/i.test(navigator.platform);

        var todaysdate = new Date().getTime();

        //mac = true;

        if(breakpointMobile != true){

          if(mac){

            $('#modal-uploader-mac').show()

          }else{

            $('#modal-uploader-windows').show()

          }

          var uploaderdate = new Date();

          uploaderdate = uploaderdate.setDate(uploaderdate.getDate()+30);

          localStorage.setItem("uploaderAlert1", "true");

          localStorage.setItem("uploaderTimestamp1", uploaderdate);

        }

      }

    }



    //This is throwing an error for some reason



    if(localStorage.getItem("alertshown2") != "true"){

      $('#heroesprofile-alert').addClass('expanded');

    }





    $('.close-window').click(function() {

      $('#heroesprofile-alert').removeClass('expanded');

      localStorage.setItem("alertshown2", "true");

    });



  }



  $('.modal-generic .close').click(function(){

    $('.modal-generic').hide();

  });

});









$('.date-format-2, .date-format').each(function(){

var originaldatehtml = $(this).html();

var originaldate = moment.tz(originaldatehtml,'Atlantic/Reykjavik');



$(this).html(originaldate.clone().local().format('MM/DD/YYYY h:mm:ss a'));

$(this).show();

$(this).removeClass('date-format-2, date-format');

});





$('.mobile-menu-button').click(function(){

  $('.menu-wrapper').slideToggle();

});









</script>
</body>
</html>
